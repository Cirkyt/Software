        mpu_set_sample_rate(st.chip_cfg.dmp_sample_rate);
         
        tmp = 0;
        mpu_write_reg(0x23, tmp);
        st.chip_cfg.dmp_on = 1;
         
        set_int_enable(1);
        mpu_reset_fifo();
    } else {
         
        set_int_enable(0);
         
        tmp = st.chip_cfg.fifo_enable;
        mpu_write_reg(0x23, tmp);
        st.chip_cfg.dmp_on = 0;
        mpu_reset_fifo();
    }
    return 0;
}





 
