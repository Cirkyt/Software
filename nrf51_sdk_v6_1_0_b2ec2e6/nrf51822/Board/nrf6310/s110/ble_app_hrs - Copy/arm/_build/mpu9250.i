{
  _Bool transfer_succeeded;
  transfer_succeeded = twi_master_transfer(m_device_address, &reg, 1, ((_Bool)0));
  transfer_succeeded &= twi_master_transfer(m_device_address|(0x01), destination, length, ((_Bool)1));
  return transfer_succeeded;
}

  
